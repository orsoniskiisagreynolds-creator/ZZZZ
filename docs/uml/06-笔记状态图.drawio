<mxfile host="app.diagrams.net">
  <diagram name="笔记状态图" id="state">
    <mxGraphModel dx="1200" dy="800" grid="1">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>
        <mxCell id="title" value="笔记生命周期 状态图" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="380" y="10" width="250" height="30" as="geometry"/>
        </mxCell>
        <!-- 初始状态 -->
        <mxCell id="start" value="" style="ellipse;fillColor=#000000" vertex="1" parent="1">
          <mxGeometry x="100" y="200" width="30" height="30" as="geometry"/>
        </mxCell>
        <!-- 状态 -->
        <mxCell id="s1" value="草稿&#xa;(status=0)" style="rounded=1;fillColor=#fff2cc;strokeColor=#d6b656" vertex="1" parent="1">
          <mxGeometry x="200" y="180" width="100" height="60" as="geometry"/>
        </mxCell>
        <mxCell id="s2" value="已发布&#xa;(status=1)" style="rounded=1;fillColor=#d5e8d4;strokeColor=#82b366" vertex="1" parent="1">
          <mxGeometry x="400" y="180" width="100" height="60" as="geometry"/>
        </mxCell>
        <mxCell id="s3" value="已归档&#xa;(status=2)" style="rounded=1;fillColor=#f5f5f5;strokeColor=#666666" vertex="1" parent="1">
          <mxGeometry x="600" y="180" width="100" height="60" as="geometry"/>
        </mxCell>
        <mxCell id="s4" value="已删除" style="rounded=1;fillColor=#f8cecc;strokeColor=#b85450" vertex="1" parent="1">
          <mxGeometry x="400" y="350" width="100" height="60" as="geometry"/>
        </mxCell>
        <!-- 终止状态 -->
        <mxCell id="end" value="" style="ellipse;fillColor=#000000;strokeWidth=3" vertex="1" parent="1">
          <mxGeometry x="700" y="365" width="30" height="30" as="geometry"/>
        </mxCell>
        <!-- 转换 -->
        <mxCell id="t1" value="创建" style="endArrow=classic;html=1" edge="1" parent="1" source="start" target="s1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t2" value="发布" style="endArrow=classic;html=1" edge="1" parent="1" source="s1" target="s2">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t3" value="归档" style="endArrow=classic;html=1" edge="1" parent="1" source="s2" target="s3">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t4" value="删除" style="endArrow=classic;html=1" edge="1" parent="1" source="s1" target="s4">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t5" value="删除" style="endArrow=classic;html=1" edge="1" parent="1" source="s2" target="s4">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t6" value="彻底删除" style="endArrow=classic;html=1" edge="1" parent="1" source="s4" target="end">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t7" value="恢复" style="endArrow=classic;html=1;dashed=1" edge="1" parent="1" source="s4" target="s1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t8" value="编辑" style="endArrow=classic;html=1;curved=1" edge="1" parent="1" source="s1" target="s1">
          <mxGeometry relative="1" as="geometry">
            <Array as="points"><mxPoint x="250" y="140"/></Array>
          </mxGeometry>
        </mxCell>
      </root>
    </mxGraphModel>
  </diagram>
</mxfile>