<mxfile host="app.diagrams.net">
  <diagram name="部署图" id="deploy">
    <mxGraphModel dx="1200" dy="800" grid="1">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>
        <mxCell id="title" value="CodeNote Learning Studio 部署图" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="380" y="10" width="300" height="30" as="geometry"/>
        </mxCell>
        <!-- 客户端 -->
        <mxCell id="client" value="客户端&#xa;(浏览器)" style="shape=cube;whiteSpace=wrap;html=1;boundedLbl=1;backgroundOutline=1;darkOpacity=0.05;darkOpacity2=0.1;fillColor=#dae8fc;strokeColor=#6c8ebf" vertex="1" parent="1">
          <mxGeometry x="100" y="200" width="120" height="80" as="geometry"/>
        </mxCell>
        <!-- Web服务器 -->
        <mxCell id="web" value="Web服务器&#xa;(Tomcat 8.5)" style="shape=cube;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366" vertex="1" parent="1">
          <mxGeometry x="350" y="150" width="150" height="180" as="geometry"/>
        </mxCell>
        <mxCell id="war" value="codenote.war&#xa;(SSM应用)" style="rounded=1;fillColor=#fff2cc;strokeColor=#d6b656" vertex="1" parent="1">
          <mxGeometry x="370" y="200" width="110" height="50" as="geometry"/>
        </mxCell>
        <mxCell id="spring" value="Spring容器" style="rounded=1;fillColor=#ffe6cc;strokeColor=#d79b00" vertex="1" parent="1">
          <mxGeometry x="370" y="260" width="110" height="30" as="geometry"/>
        </mxCell>
        <!-- 数据库服务器 -->
        <mxCell id="dbserver" value="数据库服务器&#xa;(MySQL 8.0)" style="shape=cube;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450" vertex="1" parent="1">
          <mxGeometry x="620" y="180" width="150" height="120" as="geometry"/>
        </mxCell>
        <mxCell id="dbicon" value="codenote_db&#xa;(15张表)" style="shape=cylinder3;fillColor=#1ba1e2;strokeColor=#006EAF;fontColor=#fff" vertex="1" parent="1">
          <mxGeometry x="660" y="220" width="80" height="60" as="geometry"/>
        </mxCell>
        <!-- 连接 -->
        <mxCell id="conn1" value="HTTP/HTTPS" style="endArrow=classic;startArrow=classic;html=1" edge="1" parent="1" source="client" target="web">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="conn2" value="JDBC" style="endArrow=classic;startArrow=classic;html=1" edge="1" parent="1" source="web" target="dbserver">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
      </root>
    </mxGraphModel>
  </diagram>
</mxfile>